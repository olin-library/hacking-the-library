{
  "name": "htl2.web",
  "description": "Hacking the Library web site",
  "version": "1.0.0",
  "author": "Oliver Steele <oliver.steele@olin.edu>",
  "homepage": "https://hackingthelibrary.org",
  "repository": "github:olinlibrary/htl18.org",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "build": "gatsby build",
    "data": "python3 scripts/make_events.py > data/days.json",
    "start": "gatsby develop",
    "deploy": "gatsby build && netlifyctl deploy -C netlify-local.toml -b public",
    "format": "eslint --fix --ext .jsx,.js *.js *.jsx src",
    "lint": "eslint --cache --ext .jsx,.js *.js *.jsx src",
    "reinstall": "rimraf node_modules && yarn install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^1.9.158",
    "gatsby-image": "^1.0.34",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-netlify": "^1.0.17",
    "gatsby-plugin-netlify-cms": "^1.0.5",
    "gatsby-plugin-nprogress": "^1.0.11",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-sass": "^1.0.15",
    "gatsby-plugin-sharp": "^1.6.27",
    "gatsby-remark-images": "^1.5.41",
    "gatsby-remark-katex": "^1.0.10",
    "gatsby-remark-prismjs": "^1.2.12",
    "gatsby-remark-smartypants": "^1.4.10",
    "gatsby-source-filesystem": "^1.5.14",
    "gatsby-transformer-remark": "^1.7.31",
    "gatsby-transformer-sharp": "^1.6.17",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "react-big-calendar": "^0.17.0",
    "react-helmet": "^5.2.0",
    "tachyons-sass": "^4.9.2"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gatsby-cli": "^1.1.32",
    "rimraf": "^2.6.2"
  }
}
